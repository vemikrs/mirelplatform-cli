{
  "name": "@vemijp/mirelplatform-cli",
  "version": "0.1.1",
  "type": "module",
  "bin": {
    "mirel": "dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "default": "./dist/api/index.js"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "@vemijp/mirel-shared": "workspace:*",
    "cosmiconfig": "^9.0.0",
    "ejs": "^4.0.1",
    "execa": "^9.6.1",
    "giget": "^1.2.5",
    "open": "^11.0.0",
    "prompts": "^2.4.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/prompts": "^2.4.9",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p .",
    "typecheck": "tsc --noEmit",
    "test": "echo \"(no tests yet)\""
  },
  "oclif": {
    "bin": "mirel",
    "commands": "./dist/commands"
  },
  "keywords": [
    "mirel",
    "mirelplatform",
    "oclif",
    "cli"
  ]
}
