{
  "name": "@vemijp/mirelplatform-cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "mirel": "dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "default": "./dist/api/index.js"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@oclif/core": "^4.0.0",
    "@vemijp/mirel-shared": "workspace:*",
    "cosmiconfig": "^9.0.0",
    "ejs": "^3.1.10",
    "execa": "^9.3.0",
    "giget": "^1.2.1",
    "open": "^10.2.0",
    "prompts": "^2.4.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.0",
    "@types/prompts": "^2.4.9",
    "tsx": "^4.7.0",
    "typescript": "^5.6.0"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p .",
    "typecheck": "tsc --noEmit",
    "test": "echo \"(no tests yet)\""
  },
  "oclif": {
    "bin": "mirel",
    "commands": "./dist/commands"
  },
  "keywords": [
    "mirel",
    "mirelplatform",
    "oclif",
    "cli"
  ]
}
